<ul id="links">
  <% @links.each do |link| %>
    <li> Title: <%= link.title %> URL: <%= link.url %> Tags: <% link.tags.each do |tag| %>
      <%= "#{tag.name} " %>
      <% end %>
    </li>
  <% end %>
</ul>

<form action="/links/new" method="get">
  <button type="submit" name="add_new_link" id="add_new_link">Add New Link</button>
</form>
